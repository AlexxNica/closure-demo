{
  "name": "compiler_cli_user",
  "version": "1.0.0",
  "description": "Demonstrate Angular app bundled and optimized by Google Closure Compiler",
  "main": "index.js",
  "devDependencies": {
    "@angular/common": "angular/common-builds",
    "@angular/compiler": "angular/compiler-builds",
    "@angular/compiler-cli": "angular/compiler-cli-builds",
    "@angular/core": "angular/core-builds",
    "@angular/platform-browser": "angular/platform-browser-builds",
    "@angular/platform-server": "angular/platform-server-builds",
    "@angular/tsc-wrapped": "angular/tsc-wrapped-builds",
    "google-closure-compiler": "^20170124.0.0",
    "rxjs": "5.0.1",
    "source-map-explorer": "^1.3.3",
    "tsickle": "^0.2.3",
    "typescript": "~2.1",
    "zone.js": "angular/zone.js"
  },
  "scripts": {
    "build_rxjs_es5esm": "tsc -p vendor/tsconfig.rxjs-es5+esm.json",
    "build_rxjs_es6": "tsc -p vendor/tsconfig.rxjs-es6.json",
    "build_rxjs_tsickle": "node ./node_modules/@angular/tsc-wrapped/src/main -p vendor/tsconfig.rxjs-tsickle.json",
    "postinstall": "npm run build_rxjs_es5esm",
    "build": "ngc && ./build_closure.sh",
    "explore": "source-map-explorer dist/bundle.js dist/bundle.js.map"
  },
  "author": "Alex Eagle <angular@google.com> (https://angularjs.org/)",
  "license": "MIT"
}
